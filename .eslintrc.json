{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node":true,
        "es6": true
    },
    "rules": {
        "comma-dangle":["error","never"],
        "no-unused-vars":[
            "error",
            {
                "vars": "local",
                "args": "none"
            }
          
        ],
        "linebreak-style":[2,"windows"]
      
    }
}